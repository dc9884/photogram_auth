<div class="page-header">
  <h3>My Likes</h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <% @likes.each do |like| %>
      <dt>User</dt>
      <dd><%= like.user_id %></dd>

      <dt>Photo</dt>
      <dd><%= like.photo_id %></dd>
      <% end %>
    </dl>


  </div>
</div>
